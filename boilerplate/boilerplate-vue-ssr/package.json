{
  "name": "likericebird-vue-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development npm run build --silent && cross-env NODE_ENV=development npm-run-all -p start serve:client",
    "serve:client": "vue-cli-service serve",
    "build": "npm-run-all -p build:server build:client -- --no-clean --silent",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build",
    "start": "node server/server-register",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
