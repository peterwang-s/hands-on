    "@babel/plugin-transform-exponentiation-operator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "axios": "0.18.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "koa": "2.7.0",
