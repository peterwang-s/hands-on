        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "start-a": "REACT_PROJECTS=a MobilePx2rem=37.5 PORT=3000 node scripts/start.js",
        "build-a": "REACT_PROJECTS=a MobilePx2rem=37.5 node scripts/build.js",
        "build-testing-a": "REACT_PROJECTS=a MobilePx2rem=37.5 node scripts/test-build.js",
        "start-b": "REACT_PROJECTS=b MobilePx2rem=37.5 PORT=3000 node scripts/start.js",
        "build-b": "REACT_PROJECTS=b MobilePx2rem=37.5 node scripts/build.js",
        "build-testing-b": "REACT_PROJECTS=b MobilePx2rem=37.5 node scripts/test-build.js",
        "test": "node scripts/test.js",
        "test:coverage": "node scripts/test.js --coverage",
        "lint": "yarn run lint-eslint && yarn run lint-stylelint",
        "lint-fix": "yarn run lint-eslint-fix && yarn run lint-stylelint-fix",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx src/**/*.{ts,tsx} --ignore-path .eslintignore",
        "lint-eslint-fix": "eslint -c .eslintrc.js --ext .ts,.tsx src/**/*.{ts,tsx} --ignore-path .eslintignore --fix",
        "lint-stylelint": "stylelint --config .stylelintrc.json src/**/*.styl --custom-syntax stylelint-plugin-stylus/custom-syntax",
        "lint-stylelint-fix": "stylelint --config .stylelintrc.json src/**/*.styl --custom-syntax stylelint-plugin-stylus/custom-syntax --fix",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "start-dev": "webpack"
    },
    "homepage": "https://github.com",
    "childSite": {
        "a": "https://github.com/a",
        "b": "https://github.com/b"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "json",
            "text",
            "clover",
            "cobertura"
        ],
        "reporters": [
            "default",
            "jest-junit"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": -10
            }
        },
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint -c .eslintrc.js --ignore-path .eslintignore"
        ],
        "*.{less,scss,styl}": [
            "stylelint --config .stylelintrc.json src/**/*.styl --custom-syntax stylelint-plugin-stylus/custom-syntax --fix"
        ],
        "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=10.14.2 <13"
    }
}
